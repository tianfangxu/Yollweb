server:
  port: 9999
spring:
  application:
    name: app-core
  cloud:
    config:
      #uri: http://localhost:9999
      profile: dev
      label: master
      discovery:
        enabled: true # if false use uri
        serviceId: conf-service
eureka:
  client:
    serviceUrl:
      defaultZone: http://127.0.0.1:9999/eureka/ # service register center address
  instance:
    preferIpAddress: true